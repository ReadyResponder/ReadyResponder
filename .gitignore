# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp
*~
*.kate-swp
/public/uploads
*.DS_Store

# Ignore database settings
/config/database.yml

# Ignore dotenv configuration
.env

# Ignore rspec profiling data
/spec/examples.txt

# Ignore byebug history
/.byebug_history

# Ignore Vagrant + Ansible configuration files
Vagrantfile
.vagrant/
playbook.retry
test_models.rb
Procfile
uploads/
