<h1>Listing unique_ids</h1>

<table id="unique_ids" class="generic_datatable table table-striped table-bordered">
  <thead>
    <tr>
      <th>Item</th>
      <th>Status</th>
      <th>Category</th>
      <th>Value</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @unique_ids.each do |unique_id| %>
      <tr>
        <td><%= unique_id.item %></td>
        <td><%= unique_id.status %></td>
        <td><%= unique_id.category %></td>
        <td><%= unique_id.value %></td>
        <td><%= table_button_link 'Show', unique_id %></td>
        <td><%= table_button_link 'Edit', edit_unique_id_path(unique_id) %></td>
        <td><%= table_button_link 'Destroy', unique_id, method: :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br/>

<%= independent_button_link 'New Unique', new_unique_id_path %>
<p>
  Created <%= @item_category.created_at %><br/>
Last Modified <%= @item_category.updated_at %><br/> 
Last Editor <%= @last_editor %> 
</p>