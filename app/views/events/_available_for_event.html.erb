<h3>Available (<%= @available.count %>) </h3>
<table id="available" class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th>Start</th>
      <th>End</th>
      <th>Hours</th>
      <th colspan=4 ></th>
    </tr>
  </thead>
  <tbody>
    <% @available.each do |availability| %>
      <tr>
        <td><%= link_to availability.person.fullname, availability %></td>
        <td><%= availability.person.phone %></td>
        <td><%= availability.start_time %></td>
        <td><%= availability.end_time %></td>
        <td><%#= availability.intended_duration %></td>
        <td><%#= unavailable_button(timecard) %></td>
        <td><%#= scheduled_button(timecard) %></td>
        <td><%#= worked_button(timecard) %></td>
        <td><%#= edit_button(timecard) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<p>
  Created <%= @item_category.created_at %><br/>
Last Modified <%= @item_category.updated_at %><br/> 
Last Editor <%= @last_editor %> 
</p>