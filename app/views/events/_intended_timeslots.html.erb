  <div>
    <table id="scheduled" class="table table-striped table-bordered">
      <thead>
        <tr>
          <th>Name</th>
          <th>Start</th>
          <th>End</th>
          <th>Hours</th>
        </tr>
      </thead>
      <tbody>
        <% @timecards.each do |timecard| %>
          <tr>
            <td><%= link_to timecard.person.fullname, timecard %></td>
            <td><%= l(timecard.intended_start_time)  %></td>
            <td><%= l(timecard.intended_end_time) %></td>
            <td><%= timecard.intended_duration %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div><p>
  Created <%= @item_category.created_at %><br/>
Last Modified <%= @item_category.updated_at %><br/> 
Last Editor <%= @last_editor %> 
</p>